[Unit]
Description=gunicorn daemon
After=network.target

[Service]
User=ubuntu
Group=www-data
WorkingDirectory=/home/ubuntu/new-django/todoAPP
ExecStart=gunicorn --access-logfile - --workers 3 --bind unix:/home/ubuntu/new-django/project.sock ec2-django.wsgi:application

[Install]
WantedBy=multi-user.target
